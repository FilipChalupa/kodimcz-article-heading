{
	"manifest_version": 3,
	"name": "Kodim.cz article heading",
	"description": "Stores the article heading.",
	"version": "1.0",
	"permissions": ["storage"],
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icons/320.png"
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["https://kodim.cz/*"]
		}
	]
}
