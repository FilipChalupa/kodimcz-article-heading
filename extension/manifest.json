{
	"manifest_version": 3,
	"name": "Kodim.cz article heading",
	"description": "Stores the article heading.",
	"version": "1.0",
	"permissions": ["storage"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"matches": ["https://kodim.cz/*"]
		}
	]
}
